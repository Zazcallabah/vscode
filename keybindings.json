// Place your key bindings in this file to override the defaults
[
	{
		"key": "ctrl+alt+j",
		"command": "-editor.action.insertSnippet",
		"when": "editorTextFocus && editorLangId == 'powershell'"
	},
	{
		"key": "shift+space",
		"command": "editor.action.showHover",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+k ctrl+i",
		"command": "-editor.action.showHover",
		"when": "editorTextFocus"
	}

]
